    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0-beta.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^6.2.2",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.9.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4"
  }
